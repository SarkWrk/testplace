{
  "name": "Testplace",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "$ignoreUnknownInstances": true,
      "$path": "src/shared",
      "Node" : {
        "$path": "node_modules"
      }
    },
    "ServerScriptService": {
      "$path": "src/server"
    },
    "ServerStorage": {
      "$ignoreUnknownInstances": true,
      "$path": "src/private"
    },
    "StarterGui": {
      "$path": "src/user-interface"
    },
    "StarterPlayer": {
      "StarterCharacterScripts": {
        "$path": "src/client"
      },
      "StarterPlayerScripts" : {
        "$path" : "src/character"
      }
    },
    "Workspace": {
      "$ignoreUnknownInstances": true,
      "$path": "src/workspace"
    }
  }
}